{
    "global": {
        "print_debug": false,
        "environment_name": "environmentbase",
        "monitor_stack": false,
        "write_stack_outputs": false
    },
    "template": {
        "ami_map_file": "ami_cache.json",
        "description": "Environment Generator Base deployment",
        "s3_bucket": "dualspark",
        "s3_prefix": "templates",
        "s3_upload_acl": "public-read",
        "timeout_in_minutes": "60",
        "ec2_key_default": "dualspark_rsa"
    },
    "logging": {
        "s3_bucket": "dualspark",
        "cloudtrail_log_prefix": "cloudtrail_logs",
        "elb_log_prefix": "elb_logs"
    },
    "boto": {
        "region_name": "us-west-2",
        "aws_access_key_id": null,
        "aws_secret_access_key": null
    },
    "network": {
        "network_cidr_base": "10.0.0.0",
        "network_cidr_size": "16",
        "az_count": 3,
        "subnet_types": [
            "public",
            "private"
        ],
        "subnet_config": [
            {
                "type": "public", 
                "size": "18",
                "name": "public"
            },
            {
                "type": "private", 
                "size": "22",
                "name": "private"
            },
        ],
    },
    "nat": {
        "instance_type": "t2.micro",
        "enable_ntp": false
    }
}
